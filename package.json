{
  "name": "@linkurious/eslint-config-ogma",
  "version": "2.0.2",
  "description": "Shareable ESLint 9 flat config for TypeScript projects",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js"
  },
  "scripts": {
    "postversion": "sync_versions",
    "test:unit": "echo \"Error: no test specified\"",
    "test": "eslint index.js",
    "bump:patch": "npm version patch --no-git-tag-version"
  },
  "dependencies": {
    "@eslint/js": "9.34.0",
    "eslint": "9.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-promise": "7.2.1",
    "globals": "16.3.0",
    "typescript-eslint": "8.42.0"
  },
  "peerDependencies": {
    "eslint": "9.0.0",
    "typescript": "^4.0.0 || ^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "./index.js"
  },
  "files": [
    "index.js",
    "eslint.config.js"
  ],
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "flat-config",
    "shareable-config"
  ],
  "author": "Linkurious SAS",
  "license": "Apache-2.0",
  "devDependencies": {
    "@linkurious/code-tools": "^0.0.15"
  }
}